<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="@dimen/size_dialog_padding"
        android:left="@dimen/size_dialog_padding"
        android:right="@dimen/size_dialog_padding"
        android:top="@dimen/size_dialog_padding">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorBlack50" />
            <stroke
                android:width="@dimen/size_dialog_stroke"
                android:color="@color/colorWhite" />
            <corners android:radius="@dimen/size_dialog_radius" />
        </shape>
    </item>
</layer-list>